<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yiluxiangbei.joey.haoqiu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/football256"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyAppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AboutActivity"></activity>
        <activity android:name=".activity.MatchActivity"></activity>
        <activity android:name=".activity.LoadWebActivity"></activity>
        <activity android:name=".activity.TableTeamMatchActivity"></activity>
        <activity android:name=".activity.PlayerInfoActivity"></activity>
        <activity android:name=".activity.TableMatchActivity"></activity>
        <activity android:name=".activity.SearchActivity"></activity>
    </application>

</manifest>
